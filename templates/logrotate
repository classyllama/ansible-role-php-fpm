# {{ ansible_managed_header }}
# {{ ansible_managed }}

/var/log/php-fpm/*.log {
    weekly
    missingok
    rotate 10
    compress
    delaycompress
    notifempty
    create
    sharedscripts
    postrotate
    /bin/kill -SIGUSR1 `cat /run/php-fpm/php-fpm.pid 2>/dev/null` 2>/dev/null || true
    endscript
}
